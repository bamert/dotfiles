!!Xft*dpi:96
Xft*antialias: true
Xft*rgba: rgb
Xft*hinting: true
Xft*hintstyle: hintslight
urxvt.background: #000000
urxvt.foreground: #ffffff
urxvt.scrollBar: false
URxvt*font:xft:Ubuntu Mono:pixelsize=13
URxvt*boldFont:xft:Ubuntu Mono:pixelsize=13
URxvt.keysym.C-plus: font-size:increase
URxvt.keysym.C-minus: font-size:increase
URxvt.keysym.C-equal: font-size:increase

